{
  "name": "ringri",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "ENVFILE=.env react-native run-android",
    "android:stag": "ENVFILE=.env.staging react-native run-android",
    "android:prod": "ENVFILE=.env.production react-native run-android",
    "ios": "ENVFILE=.env react-native run-ios --scheme=\"RingRi Dev\"",
    "ios:stag": "ENVFILE=.env.staging react-native run-ios --scheme=\"RingRi Staging\" --simulator='iPhone 14 Pro Max'",
    "ios:prod": "ENVFILE=.env.production react-native run-ios --scheme='RingRi'",
    "build-apk:dev": "cd android && ENVFILE=.env.dev ./gradlew assembleRelease",
    "build-apk:stag": "cd android && ENVFILE=.env.staging ./gradlew assembleRelease",
    "build-apk:prod": "cd android && ENVFILE=.env.production ./gradlew assembleRelease",
    "build-aab:dev": "cd android && ENVFILE=.env.dev ./gradlew bundleRelease",
    "build-aab:stag": "cd android && ENVFILE=.env.staging ./gradlew bundleRelease",
    "build-aab:prod": "cd android && ENVFILE=.env.production ./gradlew bundleRelease",
    "pod-install": "NO_FLIPPER=1 arch -arm64 bundle exec pod install --project-directory=ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.16",
    "@reduxjs/toolkit": "^1.9.7",
    "i18next": "^23.6.0",
    "lottie-react-native": "^6.4.0",
    "react": "18.2.0",
    "react-i18next": "^13.3.1",
    "react-native": "0.73.2",
    "react-native-bootsplash": "^5.0.4",
    "react-native-config": "^1.5.1",
    "react-native-device-info": "^10.11.0",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-mmkv": "^2.10.2",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-svg": "^13.14.0",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/runtime": "^7.23.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@react-native/metro-config": "^0.74.0",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.7",
    "@types/react": "^18.2.34",
    "@types/react-test-renderer": "^18.0.5",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "^3.2.4",
    "react-native-gradle-plugin": "^0.71.19",
    "react-native-svg-transformer": "^1.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "5.2.2"
  }
}
